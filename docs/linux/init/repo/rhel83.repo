metadata_expire=-1
gpgcheck=0
enabled=1
baseurl={{ repo_url }}/os/rhel/8.3/updates/$basearch/

[Addons]
name=Red Hat Enterprise Linux 8.3 - Addons
metadata_expire=-1
gpgcheck=0
enabled=1
baseurl={{ repo_url }}/os/rhel/8.3/addons/$basearch/
