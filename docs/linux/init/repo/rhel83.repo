enabled=1
baseurl={{ repo_url }}/os/rhel/8.3/base/$basearch/BaseOS/
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-redhat-release

[AppStream]
name=Red Hat Enterprise Linux 8.3 - AppStream
metadata_expire=-1
